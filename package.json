{
  "name": "@redactie/search-module",
  "version": "1.1.0",
  "description": "redactie search module",
  "main": "dist/redactie-search-module.umd.js",
  "module": "dist/redactie-search-module.umd.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/redactie-search-module.umd.js",
  "scripts": {
    "prepublish": "npm run test && npm run build",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run trans:extract && npm run build:types && webpack --env.prod",
    "build:w": "npm run trans:extract && npm run build:types && webpack -w --env.prod",
    "build:analyse": "npm run build:types && webpack --env.analyse",
    "start": "concurrently --kill-others \"npm run build:w\" \"cd demo && npm run start\"",
    "lint:scss": "stylelint public/**/*.scss --allow-empty-input",
    "lint:ts": "eslint --cache --cache-location .eslintcache --ext .ts,.tsx public",
    "lint": "npm run lint:scss && npm run lint:ts",
    "test": "jest ./public",
    "trans:extract": "./node_modules/.bin/i18next-scanner --config i18next-scanner.config.js",
    "docs": "npx typedoc --entryPoints ./public/index.tsx --entryPoints ./public/lib/api/index.ts --excludePrivate"
  },
  "repository": {
    "type": "git",
    "url": "https://ex02131@bitbucket.antwerpen.be/scm/wcm/redactie-search_module.git"
  },
  "keywords": [
    "Redactie",
    "React",
    "Search",
    "Module"
  ],
  "publishConfig": {
    "@redactie:registry": "https://nexusrepo.antwerpen.be/repository/npm-private"
  },
  "devDependencies": {
    "@acpaas-ui/react-components": "^5.0.9",
    "@acpaas-ui/react-editorial-components": "^1.2.0",
    "@babel/core": "7.15.5",
    "@babel/plugin-proposal-class-static-block": "^7.15.4",
    "@babel/preset-env": "7.15.6",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.15.0",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@redactie/language-module": "0.0.4-0",
    "@redactie/module-webpack-plugin": "2.0.0",
    "@redactie/redactie-core": "^2.0.0",
    "@redactie/roles-rights-module": "^1.1.1",
    "@redactie/sites-module": "^1.4.2",
    "@redactie/translations-module": "^1.3.0",
    "@redactie/utils": "^2.4.1",
    "@testing-library/jest-dom": "5.0.2",
    "@testing-library/react": "9.4.0",
    "@types/jest": "24.9.1",
    "@types/ramda": "0.26.44",
    "@types/react": "16.9.19",
    "@types/react-router-dom": "5.1.8",
    "@types/webpack-env": "^1.15.1",
    "@types/yup": "^0.26.33",
    "@typescript-eslint/eslint-plugin": "2.17.0",
    "@typescript-eslint/parser": "2.17.0",
    "babel-jest": "25.1.0",
    "clean-webpack-plugin": "3.0.0",
    "concurrently": "5.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "husky": "3.1.0",
    "i18next-scanner": "^3.1.0",
    "i18next-scanner-typescript": "^1.0.6",
    "install": "0.13.0",
    "jest": "25.1.0",
    "lint-staged": "10.0.8",
    "node-sass": "4.13.1",
    "npm": "6.13.7",
    "prettier": "1.19.1",
    "ramda": "^0.28.0",
    "react": "16.12.0",
    "react-router-dom": "5.2.0",
    "stylelint": "13.0.0",
    "stylelint-config-standard": "19.0.0",
    "ts-loader": "6.2.1",
    "typedoc": "^0.22.13",
    "typescript": "3.7.5",
    "webpack": "4.41.5",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "yup": "0.28.3"
  },
  "author": "Glenn Verschooren",
  "license": "ISC",
  "dependencies": {}
}
